# chezmoi data file for Git configuration
# This file contains machine-specific Git settings

os: "darwin"

git:
  name: "username"
  email: "username@example.com"
  editor: "vim"
  autocrlf: "input"
  defaultBranch: "main"
  pullRebase: "false"
  mergeTool: "vimdiff"
  diffTool: "vimdiff"
  excludesfile: "/Users/username/.gitignore_global"
  ghqRoot: "~/go/src"
  hubProtocol: "ssh"
